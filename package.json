{
  "name": "@leyyo/http",
  "version": "1.3.6",
  "description": "Http Mock",
  "keywords": [
    "http state",
    "http error state",
    "http success state",
    "http redirect state",
    "http protocol",
    "http mock",
    "request mock",
    "response mock"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leyyonet/http.git"
  },
  "bugs": {
    "url": "https://github.com/leyyonet/http.git/issues",
    "email": "mustafayelmer@gmail.com"
  },
  "homepage": "https://github.com/leyyonet/http.git/#readme",
  "author": "Mustafa Yelmer <mustafayelmer@gmail.com>",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clear": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --check \"{src,test}/**/*.{ts,tsx,js}\"",
    "format:force": "prettier --write \"{src,test}/**/*.{ts,tsx,js}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "asset": "node -r ts-node/register commands/assets.js",
    "build": "npm run clear && tsc && npm run asset",
    "publish:public": "npm run lint && npm run format:force && npm run test && npm run build && npm publish --access=public"
  },
  "files": [
    "dist/*"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/mime-types": "^3.0.1",
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.2.1",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.24.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "mime-types": "^3.0.2",
    "@leyyo/common": "^1.3.19"
  }
}
