{
  "name": "@leyyo/http-mock",
  "version": "1.3.4",
  "description": "Http Mock",
  "keywords": [
    "http mock",
    "request mock",
    "response mock"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leyyonet/http-mock.git"
  },
  "bugs": {
    "url": "https://github.com/leyyonet/http-mock.git/issues",
    "email": "mustafayelmer@gmail.com"
  },
  "homepage": "https://github.com/leyyonet/http-mock.git/#readme",
  "author": "Mustafa Yelmer <mustafayelmer@gmail.com>",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clear": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "asset": "node -r ts-node/register commands/assets.js",
    "build": "npm run clear && tsc && npm run asset",
    "test": "npx jest --config=jest.json --clearCache && npx jest --config=jest.json --verbose",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "~publish": "npm run build && npm publish -access=public"
  },
  "files": [
    "dist/*"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@types/express": "^5.0.6",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "mime-types": "^3.0.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@leyyo/common": "^1.3.11"
  }
}
